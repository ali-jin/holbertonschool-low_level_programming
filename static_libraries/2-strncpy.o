#include <stdio.h>
#include <string.h>
#include "main.h"

/**
 * _strncpy - Entry point
 * @dest: pointer string
 * @src: pointer string
 * @n: integer
 *
 * Return: dest (Success)
 */
char *_strncpy(char *dest, char *src, int n)
{
	int i;

	for (i = 0; i < n && src[i] != '\0'; i++)
		dest[i] = src[i];
	for ( ; i < n; i++)
		dest[i] = '\0';
}
